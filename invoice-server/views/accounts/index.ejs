<!DOCTYPE html>
<html>
    <body>
        <h1>Accounts</h1>
        <table>
            <tr>
                <th>BSB</th>
                <th>Account No.</th>
                <th>Account Name</th>
            </tr>
            <% accounts.forEach(function(account) { %>
            <tr>
                <td><%= account.bsb %></td>
                <td><%= account.number %></td>
                <td><%= account.name %></td>
            </tr>
            <% }); %>
        </table>
    </body>
</html>
