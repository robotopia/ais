<!DOCTYPE html>
<html>
    <head>
        <link href="/css/style.css" rel="stylesheet" type="text/css">
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css">
    </head>
    <body>
        <h1>Invoices</h1>
        <table class="mytable edittable">
            <tr>
                <th>Month/Year</th>
                <th>Created</th>
                <th>Issued</th>
                <th>Due</th>
                <th>Total amount</th>
                <th>Paid</th>
                <th>PDF</th>
            </tr>
            <% invoices.forEach(function(invoice) { %>
            <tr>
                <td><a href="invoice/<%= invoice.year %>/<%= invoice.month %>"><%= invoice.month %> / <%= invoice.year %><a></td>
                <td><%= invoice.created_str %></td>
                <td><%= invoice.issued_str %></td>
                <td><%= invoice.due_str %></td>
                <td>$<%= invoice.total_amount %></td>
                <td><%= invoice.paid_str %></td>
                <td><%= invoice.pdf %></td>
            </tr>
            <% }); %>
        </table>
    </body>
</html>
